{
  "uid": "url-shortener",
  "title": "URL Shortener â€“ Overview",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Requests per second",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(url_shortener_http_requests_total[1m]))",
          "legendFormat": "RPS",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 6 }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "HTTP latency (p50 / p95)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum(rate(url_shortener_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(url_shortener_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        }
      ],
      "gridPos": { "x": 0, "y": 6, "w": 24, "h": 6 }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "HTTP responses by status",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(url_shortener_http_requests_total[1m])) by (status)",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 12, "w": 24, "h": 6 }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Rate-limit blocked requests",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(url_shortener_ratelimit_blocked_total[1m])) by (scope)",
          "legendFormat": "{{scope}}",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 18, "w": 24, "h": 6 }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Service availability",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "up{job=\"url-shortener\"}",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "value",
        "graphMode": "none"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "1": { "text": "UP", "color": "green" },
                "0": { "text": "DOWN", "color": "red" }
              }
            }
          ]
        }
      },
      "gridPos": { "x": 0, "y": 24, "w": 24, "h": 4 }
    }
  ]
}
